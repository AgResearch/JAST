universe=java
arguments = jast.JASTmain --bowmap bowMap --bowbuild bowBuild -l librairy.txt -r reads_1.fastq -p reads_2.fastq -o outputName --colombus colombusFile --ref reference.fasta --sspace sspacefile --flexbar flexbarFile
